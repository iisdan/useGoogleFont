{
	"name": "@danherbert/use-google-font",
	"version": "1.0.11",
	"description": "A React hook to load Google Fonts",
	"main": "dist/useGoogleFont.js",
	"module": "dist/useGoogleFont.mjs",
	"types": "dist/useGoogleFont.d.ts",
	"scripts": {
		"build": "tsup src/useGoogleFont.ts --format cjs,esm --dts",
		"test": "echo \"Error: no test specified\" && exit 1",
	},
	"author": "Dan Herbert",
	"license": "MIT",
	"devDependencies": {
		"@swc/core": "^1.10.7",
		"@types/react": "^19.0.7",
		"react": "^19.0.0",
		"tsup": "^6.7.0",
		"typescript": "^5.0.0"
	},
	"peerDependencies": {
		"react": "^17.0.0 || ^18.0.0 || ^19.0.0"
	},
	"dependencies": {
		"react": "^17.0.0 || ^18.0.0 || ^19.0.0"
	},
	"tsup": {
		"entry": [
			"src/useGoogleFont.ts"
		],
		"format": [
			"cjs",
			"esm"
		],
		"dts": true,
		"external": [
			"react"
		]
	}
}
