{
	"name": "@danherbert/use-google-font",
	"version": "1.0.14",
	"description": "A React hook to load Google Fonts",
	"main": "dist/useGoogleFont.js",
	"module": "dist/useGoogleFont.mjs",
	"types": "dist/useGoogleFont.d.ts",
	"repository": {
		"type": "git",
		"url": "https://github.com/iisdan/useGoogleFont.git"
	},
	"scripts": {
		"build": "tsup src/useGoogleFont.ts --format cjs,esm --dts",
		"test": "jest"
	},
	"author": "Dan Herbert",
	"license": "MIT",
	"devDependencies": {
		"@testing-library/jest-dom": "^6.6.3",
		"@testing-library/react": "^14.2.1",
		"@types/jest": "^29.5.14",
		"@types/react": "^19.0.7",
		"@types/react-dom": "^19.0.3",
		"jest": "^29.7.0",
		"jest-environment-jsdom": "^29.7.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"ts-jest": "^29.2.5",
		"ts-node": "^10.9.2",
		"tsup": "^6.7.0",
		"typescript": "^5.0.0"
	},
	"peerDependencies": {
		"react": "^17.0.0 || ^18.0.0 || ^19.0.0"
	},
	"dependencies": {
		"react": "^17.0.0 || ^18.0.0 || ^19.0.0"
	},
	"tsup": {
		"entry": [
			"src/useGoogleFont.ts"
		],
		"format": [
			"cjs",
			"esm"
		],
		"dts": true,
		"external": [
			"react"
		]
	}
}
